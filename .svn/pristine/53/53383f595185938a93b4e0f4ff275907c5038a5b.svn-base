import request from '@/utils/request'
// 获得所有高度相似作品
export function getSimilarWorksMes(data) {
  return request({
    url: '/api/getSimilarWorksMes',
    method: 'post',
    params: {
      pageNum: data.page,
      pageSize: data.limit
    }
  })
}

export function passSimilar(data) {
  return request({
    url: '/api/ShenHeSimilarWork',
    method: 'post',
    params: {
      widList: data
    }
  })
}

export function returnSimilar(data) {
  return request({
    url: '/api/TuiHuiSimilarWork',
    method: 'post',
    params: {
      widList: data.wid,
      subject: data.theme,
      content: data.content
    }
  })
}

export function getSimilarWorksMesByWid(id) {
  return request({
    url: '/api/getSimilarWorksMesByWid',
    method: 'post',
    params: {
      wid: id
    }
  })
}
