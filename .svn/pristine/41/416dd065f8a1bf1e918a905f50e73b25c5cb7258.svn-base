import request from '@/utils/request'

// 获得待审核作品
export function getFinalWorks(data) {
  return request({
    url: '/api/getReadyWorks',
    method: 'post',
    params: {
      pageNum: data.page,
      pageSize: data.limit,
      match_name: data.match_name,
      match_project: data.match_project,
      type: data.type
    }
  })
}

// 获得奖项信息
export function getPrizeName() {
  return request({
    url: '/api/getPrizeNameWithID',
    method: 'post',
    params: {

    }
  })
}

export function advance(list) {
  return request({
    url: '/api/toJSWorks',
    method: 'post',
    params: {
      wid: list.wid,
      certificate: list.tempRadio,
      reward_date: list.time
    }
  })
}
