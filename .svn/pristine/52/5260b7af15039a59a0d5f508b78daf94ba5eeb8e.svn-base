import request from '@/utils/request'

export function getUnrise(data) {
  return request({
    url: '/api/getNoCXWorksByMid',
    method: 'post',
    params: {
      mid: '',
      pageNum: data.page,
      pageSize: data.limit
    }
  })
}

export function Audit(wid) {
  return request({
    url: '/api/toShenHeCXWorks',
    method: 'post',
    params: {
      wid: wid
    }
  })
}

export function Return(mid) {
  return request({
    url: '/api/toTuihuiCXworks',
    method: 'post',
    params: {
      mid: mid
    }
  })
}
