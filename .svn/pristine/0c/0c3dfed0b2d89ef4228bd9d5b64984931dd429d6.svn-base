import request from '@/utils/request'

export function returnNoscored(data) {
  return request({
    url: '/api/toTuiHuiPF',
    method: 'post',
    params: {
      widList: data.wid,
      subject: data.theme,
      content: data.content
    }
  })
}

export function getNoscored(list) {
  return request({
    url: '/api/getNoScoredWorks',
    method: 'post',
    params: {
      pageNum: list.page,
      pageSize: list.limit
    }
  })
}

export function noscoredSearch(data) {
  return request({
    url: '/api/getWorksMesCByString',
    method: 'post',
    params: {
      pageNum: data.pageNum,
      pageSize: data.pageSize,
      match_name: data.match_name,
      match_project: data.match_project,
      work_name: data.work_name,
      type: data.type,
      teamname: data.teamname,
      author1_name: data.author1_name,
      teacher: data.teacher,
      start_date: data.start_date,
      end_date: data.end_date
    }
  })
}
